{
  "name": "product-service",
  "version": "0.1.0",
  "bin": {
    "product-service": "bin/product-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:deploy": "npx cdk deploy --profile yanaadmin",
    "cdk:destroy": "npx cdk destroy --profile yanaadmin",
    "rds:create": "npx ts-node  --transpile-only ./node_modules/.bin/knex migrate:latest --knexfile ./db/knexfile.ts --cwd ./",
    "rds:drop": "npx ts-node --transpile-only ./node_modules/.bin/knex migrate:rollback --knexfile ./db/knexfile.ts --cwd ./",
    "rds:fill": "npx ts-node ./node_modules/.bin/knex seed:run --knexfile ./db/knexfile.ts"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.86.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.82.0-alpha.0",
    "@aws-sdk/client-sns": "^3.359.0",
    "@aws-sdk/lib-dynamodb": "^3.350.0",
    "@types/aws-lambda": "^8.10.115",
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "@types/uuid": "^9.0.2",
    "aws-cdk": "2.81.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.82.0-alpha.0",
    "@aws-sdk/client-dynamodb": "^3.348.0",
    "aws-cdk-lib": "2.81.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.1.4",
    "knex": "^2.4.2",
    "pg": "^8.11.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
